{
  "name": "cucumber-docx",
  "description": "express API endpoint to send docx files containing vars + dict context and returns a docx with the vars replaced. It uses docxtemplater node library.",
  "version": "0.1.0",
  "engines": {
    "node": "6.10.0"
  },
  "main": "./bin/www.js",
  "scripts": {
    "start": "node $NODE_DEBUG_OPTION ./bin/www.js"
  },
  "dependencies": {
    "body-parser": "~1.17.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.3",
    "docxtemplater": "^3.1.7",
    "dotenv": "^4.0.0",
    "express": "~4.15.2",
    "formidable": "^1.1.1",
    "fs": "0.0.1-security",
    "http": "0.0.0",
    "jade": "~1.11.0",
    "jszip": "^2.6.1",
    "morgan": "~1.8.1",
    "newrelic": "^2.6.1",
    "path": "^0.12.7",
    "raven": "^2.2.1",
    "serve-favicon": "~2.4.2",
    "winston": "^2.4.0",
    "winston-raven-sentry": "^1.0.1",
    "winston-sentry": "^0.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/reclamador/cucumber_docx"
  },
  "keywords": [
    "node",
    "express",
    "docker",
    "docx",
    "parser",
    "docxparser"
  ],
  "license": "MIT"
}
